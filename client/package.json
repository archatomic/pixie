{
  "name": "pixie-client",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.18.10",
    "@fortawesome/fontawesome-free": "^6.2.0",
    "@parcel/babel-preset-env": "^2.7.0",
    "@types/jest": "^28.1.7",
    "classnames": "^2.3.1",
    "immutable": "^4.1.0",
    "jest": "^28.1.3",
    "nanoid": "^4.0.0",
    "parcel": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.2",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.0"
  },
  "scripts": {
    "start": "parcel ./index.html --open",
    "build": "parcel build ./index.html --dist-dir ../dist --no-optimize",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@parcel/transformer-stylus": "2.7.0",
    "jest-environment-jsdom": "^29.1.1",
    "process": "^0.11.10"
  },
  "alias": {
    "Pixie": "./src"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [],
    "transform": {
      "\\.js$": [
        "babel-jest",
        {
          "presets": [
            "@parcel/babel-preset-env"
          ]
        }
      ]
    },
    "moduleNameMapper": {
      "Pixie/(.*)$": "<rootDir>/src/$1"
    }
  }
